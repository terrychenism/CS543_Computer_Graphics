#include <iostream>
#include <ctime>
#include <cstdlib>
using namespace std;
 
int main()
{
    double random(double,double);
    srand(unsigned(time(0)));
    for(int icnt = 0; icnt != 3; ++icnt)
        cout << "No." << icnt+1 << ": " << int(random(0,512))<< endl; 
	getchar();
    return 0;
}
double random(double start, double end)
{ return start+(end-start)*rand()/(RAND_MAX+ 1.0);}
